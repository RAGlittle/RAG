    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug dataloader",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/rag-gateway/cmd/dataloader",
            "args": [
                "-d",
                "${workspaceFolder}/data/westernblot/pdf"
            ],
        },
        {
            "name": "Debug Gateway",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/rag-gateway/cmd/gateway",
            "args": [
                "-f",
                "${workspaceFolder}/rag-gateway/config.yaml"
            ],
        },
    ]
}